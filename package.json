    "jest-cli": "^0.5.0",
    "jsx-loader": "^0.12.2",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "requirejs": "~2.1.9",
    "webpack": "^1.5.1",
    "webpack-dev-server": "^1.7.0"
