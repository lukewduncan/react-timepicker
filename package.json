{
  "name": "react-timepicker",
  "version": "0.0.1",
  "description": "A React.js timepicker component",
  "homepage": "https://github.com/lukewduncan/react-timepicker",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukewduncan/react-timepicker"
  },
  "main": "./src/timepicker.js.jsx",
